<div class="container">
	<h2> Products </h2>
	<div class="row">
		<div class="col-md-12">
			<div class="row" >
			{{# each products}}
				<div class="col-md-4">
					<h3><a href="{{productUrl slug}}">{{{title}}}</a></h3>
					{{#if image}}
						<img src="{{{cloudinaryUrl image width=240 height=240 crop="fit"}}}" class="thumbnail">

					{{/if}}
				</div>
			{{/each}}
		</div>
	</div>
</div>